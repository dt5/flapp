<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alduran.doranwalsten.flapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/flapp_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewFaceActivity"></activity>
        <activity android:name=".MainDesign"></activity>
        <activity android:name=".RhomboidActivity"></activity>
        <activity android:name=".MainFeedbackActivity"></activity>

    </application>

</manifest>
